{
  "name": "Time Waste Tracker",
  "version": "0.1",
  "description": "Time Waste Tracker",
  "manifest_version": 2,
  "permissions": ["declarativeContent", "storage"], 
  "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
          
